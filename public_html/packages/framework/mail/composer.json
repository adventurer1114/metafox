{
    "name": "metafox/mail",
    "version": "5.0.3",
    "description": "Mail Sender",
    "keyword": "",
    "authors": [
        {
            "name": "phpFox",
            "homepage": "https://www.phpfox.com"
        }
    ],
    "extra": {
        "metafox": {
            "core": true,
            "priority": 9,
            "alias": "mail",
            "asset": "mail",
            "namespace": "MetaFox\\Mail",
            "path": "packages/framework/mail",
            "title": "Mail Sender",
            "providers": [
                "MetaFox\\Mail\\Providers\\PackageServiceProvider"
            ],
            "aliases": null,
            "frontend": [],
            "internalUrl": "/mail",
            "internalAdminUrl": "/admincp/mail/setting",
            "type": "app",
            "mobile": [],
            "store_id": 0,
            "store_url": null
        }
    },
    "require": {
        "symfony/mailgun-mailer": "^6.2",
        "symfony/http-client": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "MetaFox\\Mail\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MetaFox\\Mail\\Tests\\": "tests/"
        }
    }
}
